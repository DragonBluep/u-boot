/dts-v1/;

#include "mt7628a.dtsi"

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>
#include <dt-bindings/leds/common.h>

/ {
	compatible = "hiwifi,hc5661a", "mediatek,mt7628-soc";
	model = "HiWiFi HC5661A";

	aliases {
		serial0 = &uart0;
		spi0 = &spi0;
	};

	chosen {
		stdout-path = &uart0;
	};

	config {
		boot-btn1 = "reset";
		boot-led1 = "blue:status";
		bootcmd_1-1 = "tftp_load_initramfs_loop";
		bootcmd_1-2 = "tftp_restore_system_loop";
		bootcmd_1-3 = "setup_netconsole";
	};

	gpio-keys {
		compatible = "gpio-keys";

		button-reset {
			label = "reset";
			linux,code = <KEY_RESTART>;
			gpios = <&gpio1 6 GPIO_ACTIVE_LOW>;
		};
	};

	gpio-leds {
		compatible = "gpio-leds";

		led-status {
			color = <LED_COLOR_ID_BLUE>;
			function = LED_FUNCTION_STATUS;
			gpios = <&gpio1 5 GPIO_ACTIVE_HIGH>;
			default-state = "on";
		};

		led-internet {
			color = <LED_COLOR_ID_BLUE>;
			function = "internet";
			gpios = <&gpio0 1 GPIO_ACTIVE_HIGH>;
			default-state = "off";
		};

		led-wlan {
			color = <LED_COLOR_ID_BLUE>;
			function = LED_FUNCTION_WLAN;
			gpios = <&gpio1 12 GPIO_ACTIVE_HIGH>;
			default-state = "off";
		};
	};
};

&eth {
	mediatek,wan-port = <4>;

	pinctrl-names = "default";
	pinctrl-0 = <&ephy_router_mode>;
};

&mmc {
	status = "okay";

	bus-width = <4>;
	cap-sd-highspeed;
};

&spi0 {
	flash@0 {
		compatible = "jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <40000000>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "u-boot";
				reg = <0x000000 0x030000>;
			};

			partition@30000 {
				label = "u-boot-env";
				reg = <0x030000 0x010000>;
			};

			partition@40000 {
				label = "factory";
				reg = <0x040000 0x010000>;
			};

			partition@50000 {
				label = "firmware";
				reg = <0x50000 0xf70000>;
			};

			partition@fc0000 {
				label = "oem";
				reg = <0xfc0000 0x020000>;
			};

			partition@fe0000 {
				label = "bdinfo";
				reg = <0xfe0000 0x010000>;
			};

			partition@ff0000 {
				label = "backup";
				reg = <0xff0000 0x010000>;
			};
		};
	};
};

&state_default {
	gpios {
		groups = "spis", "spi cs1", "i2s", "wdt", "perst", "refclk", \
			 "uart2", "pwm0", "pwm1", "wled_a", "p0led_a", "p1led_a", \
			 "p2led_a", "p3led_a", "p4led_a";
		function = "gpio";
	};

	/* sd router mode */
	sdxc_router_mode {
		groups = "sd router";
		function = "router";
	};

	sdxc_map_pins {
		groups = "gpio0", "sdmode", "i2c", "uart1";
		function = "gpio";
	};

	sdxc_clk_pad {
		pins = "gpio0";
		drive-strength-28 = <8>;
	};
};
